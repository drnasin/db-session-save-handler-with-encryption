{
  "name": "drnasin/mysql-pdo-secure-session-handler",
  "version": "1.5.0",
  "license": "MIT",
  "type": "library",
  "description": "Mysql secure session handler with AES-256-CTR encryption of session data and 'per session' based lifetime.",
  "keywords": [
    "php",
    "mysql",
    "session",
    "secure",
    "pdo",
    "session handler",
    "encryption",
    "encrypted",
    "session data"
  ],
  "homepage": "https://github.com/drnasin/mysql-pdo-secure-session-handler",
  "authors": [
    {
      "name": "Ante Drnasin",
      "email": "ante.drnasin@gmail.com",
      "homepage": "http://www.drnasin.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-pdo_mysql": ">=5.6",
    "ext-openssl": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Drnasin\\": "src/Drnasin/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload-dev": {
    "psr-4": {
      "Drnasin\\": "tests/src/Drnasin/"
    }
  },
  "scripts": {
    "tests": "phpunit -c tests/phpunit.xml"
  }
}
